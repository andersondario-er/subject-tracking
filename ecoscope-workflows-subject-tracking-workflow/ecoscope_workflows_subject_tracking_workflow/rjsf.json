      "type": "object"
    },
    "TemporalGrouper": {
      "properties": {
        "index_name": {
          "title": "Index Name",
          "type": "string"
        },
        "directive": {
          "enum": [
            "%a",
            "%A",
            "%b",
            "%B",
            "%c",
            "%d",
            "%f",
            "%H",
            "%I",
            "%j",
            "%m",
            "%M",
            "%p",
            "%S",
            "%U",
            "%w",
            "%W",
            "%x",
            "%X",
            "%y",
            "%Y",
            "%z",
            "%%"
          ],
          "title": "Directive",
          "type": "string"
        }
      },
      "required": [
        "index_name",
        "directive"
      ],
      "title": "TemporalGrouper",
      "type": "object"
    },
    "TimeRange": {
      "properties": {
        "since": {
          "format": "date-time",
